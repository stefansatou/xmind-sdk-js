{
  "name": "xmind",
  "version": "2.2.24",
  "description": "The SDK of XMind Ltd.",
  "main": "dist/index.js",
  "browser": "dist/xmind-sdk.bundle.js",
  "scripts": {
    "test": "npm run lint && nyc mocha",
    "build": "npm run test && rm -rf ./dist && node_modules/.bin/tsc && npm run cp && npm run browserify",
    "lint": "node_modules/.bin/tslint --config tslint.json --project .",
    "fix": "node_modules/.bin/tslint --config tslint.json --project . --fix",
    "build-doc": "BUILD_DOCS=true node scripts/icons.js",
    "browserify": "node scripts/browserify.js",
    "cov": "cat ./coverage/lcov.info | codacy-coverage --language typescript",
    "clean": "rm -rf ./coverage ./.nyc_output",
    "cp": "cp -r ./src/common/templates ./src/common/themes ./dist/common/",
    "patch": "npm run build && npm version patch && npm publish --access public",
    "minor": "npm run build && npm version minor && npm publish --access public",
    "major": "npm run build && npm version major && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xmindltd/xmind-sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/xmindltd/xmind-sdk-js/issues"
  },
  "keywords": [
    "xmind-sdk",
    "xmindsdk",
    "xmind",
    "xmind-sdk-js",
    "xmind-sdk-ts",
    "xmind-package"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "ajv": "^6.10.0",
    "debug": "^4.1.1",
    "jszip": "^3.2.1",
    "uuid": "^3.3.2",
    "xmind-model": "^1.1.12"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/ajv": "^1.0.0",
    "@types/chai": "^4.1.7",
    "@types/debug": "^4.1.4",
    "@types/jszip": "^3.1.5",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/eslint-plugin-tslint": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "browser-pack-flat": "^3.4.1",
    "browserify": "^16.2.3",
    "chai": "^4.2.0",
    "codacy-coverage": "^3.4.0",
    "common-shakeify": "^0.6.0",
    "envify": "^4.1.0",
    "istanbul": "^0.4.5",
    "minify-stream": "^1.2.0",
    "mocha": "^6.1.4",
    "mocha-appveyor-reporter": "^0.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "source-map-support": "^0.5.12",
    "ts-node": "^10.5.0",
    "tsify": "^4.0.1",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "uglify-js": "^3.6.0",
    "uglifyify": "^5.0.1",
    "unassertify": "^2.1.1"
  },
  "author": "daniels",
  "license": "MIT"
}
